using System;

class Program
{
    static void Main()
    {
        Console.Write("Enter the number for the multiplication table: ");
        int number = Convert.ToInt32(Console.ReadLine());

        Console.WriteLine($"Multiplication Table for {number}:");
        for (int i = 1; i <= 10; i++)
        {
            Console.WriteLine($"{number} x {i} = {number * i}");
        }
    }
}
